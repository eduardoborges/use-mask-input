{
  "name": "use-mask-input-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for use-mask-input",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=vite-project",
    "dev:next": "turbo run dev --filter=next-project",
    "dev:docs": "turbo run start --filter=docs",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/react": ">=17",
    "@types/react-dom": ">=17",
    "turbo": "^2.3.3",
    "typescript": "5.1"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "packageManager": "npm@10.0.0"
}
