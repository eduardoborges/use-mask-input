{
  "name": "use-mask-input-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for use-mask-input",
  "repository": {
    "url": "https://github.com/eduardoborges/use-mask-input"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=vite-project",
    "dev:next": "turbo run dev --filter=next-project",
    "dev:docs": "turbo run start --filter=docs",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "turbo": "^2.7.6",
    "typescript": "5.9",
    "wrangler": "^4.60.0"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@10.28.2"
}